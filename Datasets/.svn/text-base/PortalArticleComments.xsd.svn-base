<?xml version="1.0" encoding="utf-8" ?><xs:schema id="PortalArticleCommentsDS" targetNamespace="http://tempuri.org/PortalArticleCommentsDS.xsd" elementFormDefault="qualified"	attributeFormDefault="qualified" xmlns="http://tempuri.org/PortalArticleCommentsDS.xsd" xmlns:mstns="http://tempuri.org/PortalArticleCommentsDS.xsd"	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" xmlns:msprop="urn:schemas-microsoft-com:xml-msprop" >	<xs:element name="PortalArticleCommentsDS" msdata:IsDataSet="true">		<xs:complexType>			<xs:choice maxOccurs="unbounded">				<xs:element name="PortalArticleComments">					<xs:complexType>						<xs:sequence>				<xs:element name="Comment" >				    <xs:simpleType>				        <xs:restriction base="xs:string" >				            <xs:maxLength value="450"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="Title" >				    <xs:simpleType>				        <xs:restriction base="xs:string" >				            <xs:maxLength value="150"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="LastEditedDate" type="xs:dateTime" />				<xs:element name="PublishedDate" type="xs:dateTime" />				<xs:element name="UserName"  minOccurs="0" msprop:nullValue="_null">				    <xs:simpleType>				        <xs:restriction base="xs:string">				            <xs:maxLength value="150"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="ArticleID" type="xs:long" />				<xs:element name="ArticleCommentID" msdata:ReadOnly="true" msdata:AutoIncrement="true" type="xs:long" />				<xs:element name="UserID" type="xs:long" />				<xs:element name="avatarurl"  minOccurs="0" msprop:nullValue="_null">				    <xs:simpleType>				        <xs:restriction base="xs:string">				            <xs:maxLength value="250"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>						</xs:sequence>					</xs:complexType>				</xs:element>			</xs:choice>		</xs:complexType>		<xs:unique name="PKPortalArticleComments" msdata:PrimaryKey="true">			<xs:selector xpath=".//mstns:PortalArticleComments" />			<xs:field xpath="mstns:ArticleCommentID" />		</xs:unique>	</xs:element></xs:schema>