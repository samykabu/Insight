<?xml version="1.0" encoding="utf-8" ?><xs:schema id="ForumsForumDS" targetNamespace="http://tempuri.org/ForumsForumDS.xsd" elementFormDefault="qualified"	attributeFormDefault="qualified" xmlns="http://tempuri.org/ForumsForumDS.xsd" xmlns:mstns="http://tempuri.org/ForumsForumDS.xsd"	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" xmlns:msprop="urn:schemas-microsoft-com:xml-msprop" >	<xs:element name="ForumsForumDS" msdata:IsDataSet="true">		<xs:complexType>			<xs:choice maxOccurs="unbounded">				<xs:element name="ForumsForum">					<xs:complexType>						<xs:sequence>				<xs:element name="Name" >				    <xs:simpleType>				        <xs:restriction base="xs:string" >				            <xs:maxLength value="50"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="Description" >				    <xs:simpleType>				        <xs:restriction base="xs:string" >				            <xs:maxLength value="255"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="NumPosts" type="xs:int" />				<xs:element name="SortOrder" type="xs:int" />				<xs:element name="NumTopics" type="xs:int" />				<xs:element name="RemoteURL"  minOccurs="0" msprop:nullValue="_null">				    <xs:simpleType>				        <xs:restriction base="xs:string">				            <xs:maxLength value="150"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="ParentID" type="xs:long"  minOccurs="0"/>				<xs:element name="LastPosted" type="xs:dateTime"  minOccurs="0"/>				<xs:element name="LastTopicID" type="xs:long"  minOccurs="0"/>				<xs:element name="Flags" type="xs:int"  minOccurs="0"/>				<xs:element name="CategoryID" type="xs:long" />				<xs:element name="LastMessageID" type="xs:long"  minOccurs="0"/>				<xs:element name="ThemeURL"  minOccurs="0" msprop:nullValue="_null">				    <xs:simpleType>				        <xs:restriction base="xs:string">				            <xs:maxLength value="50"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="ForumID" msdata:ReadOnly="true" msdata:AutoIncrement="true" type="xs:long" />				<xs:element name="LastUserName"  minOccurs="0" msprop:nullValue="_null">				    <xs:simpleType>				        <xs:restriction base="xs:string">				            <xs:maxLength value="50"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>						</xs:sequence>					</xs:complexType>				</xs:element>			</xs:choice>		</xs:complexType>		<xs:unique name="PKForumsForum" msdata:PrimaryKey="true">			<xs:selector xpath=".//mstns:ForumsForum" />			<xs:field xpath="mstns:ForumID" />		</xs:unique>	</xs:element></xs:schema>