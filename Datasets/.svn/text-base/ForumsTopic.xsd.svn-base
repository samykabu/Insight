<?xml version="1.0" encoding="utf-8" ?><xs:schema id="ForumsTopicDS" targetNamespace="http://tempuri.org/ForumsTopicDS.xsd" elementFormDefault="qualified"	attributeFormDefault="qualified" xmlns="http://tempuri.org/ForumsTopicDS.xsd" xmlns:mstns="http://tempuri.org/ForumsTopicDS.xsd"	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" xmlns:msprop="urn:schemas-microsoft-com:xml-msprop" >	<xs:element name="ForumsTopicDS" msdata:IsDataSet="true">		<xs:complexType>			<xs:choice maxOccurs="unbounded">				<xs:element name="ForumsTopic">					<xs:complexType>						<xs:sequence>				<xs:element name="UserName" >				    <xs:simpleType>				        <xs:restriction base="xs:string" >				            <xs:maxLength value="50"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="numPosts" type="xs:long" />				<xs:element name="ForumID" type="xs:long" />				<xs:element name="isQuestion" type="xs:boolean"  minOccurs="0"/>				<xs:element name="Topic" >				    <xs:simpleType>				        <xs:restriction base="xs:string" >				            <xs:maxLength value="150"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="LastUserName"  minOccurs="0" msprop:nullValue="_null">				    <xs:simpleType>				        <xs:restriction base="xs:string">				            <xs:maxLength value="50"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="AnswerMessageID" type="xs:long"  minOccurs="0"/>				<xs:element name="LastUserID" type="xs:long" />				<xs:element name="PollID" type="xs:long"  minOccurs="0"/>				<xs:element name="flags" type="xs:int" />				<xs:element name="isDeleted" type="xs:boolean"  minOccurs="0"/>				<xs:element name="LastMessageID" type="xs:long" />				<xs:element name="UserID" type="xs:long" />				<xs:element name="Posted" type="xs:dateTime" />				<xs:element name="TopicID" msdata:ReadOnly="true" msdata:AutoIncrement="true" type="xs:long" />				<xs:element name="TopicMovedID" type="xs:long"  minOccurs="0"/>				<xs:element name="Priority" type="xs:int" />				<xs:element name="LastPosted" type="xs:dateTime" />				<xs:element name="Views" type="xs:long" />						</xs:sequence>					</xs:complexType>				</xs:element>			</xs:choice>		</xs:complexType>		<xs:unique name="PKForumsTopic" msdata:PrimaryKey="true">			<xs:selector xpath=".//mstns:ForumsTopic" />			<xs:field xpath="mstns:TopicID" />		</xs:unique>	</xs:element></xs:schema>