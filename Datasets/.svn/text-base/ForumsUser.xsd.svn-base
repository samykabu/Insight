<?xml version="1.0" encoding="utf-8" ?><xs:schema id="ForumsUserDS" targetNamespace="http://tempuri.org/ForumsUserDS.xsd" elementFormDefault="qualified"	attributeFormDefault="qualified" xmlns="http://tempuri.org/ForumsUserDS.xsd" xmlns:mstns="http://tempuri.org/ForumsUserDS.xsd"	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" xmlns:msprop="urn:schemas-microsoft-com:xml-msprop" >	<xs:element name="ForumsUserDS" msdata:IsDataSet="true">		<xs:complexType>			<xs:choice maxOccurs="unbounded">				<xs:element name="ForumsUser">					<xs:complexType>						<xs:sequence>				<xs:element name="Joined" type="xs:dateTime" />				<xs:element name="TimeZone" type="xs:int" />				<xs:element name="Email"  minOccurs="0" msprop:nullValue="_null">				    <xs:simpleType>				        <xs:restriction base="xs:string">				            <xs:maxLength value="50"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="ThemeFile"  minOccurs="0" msprop:nullValue="_null">				    <xs:simpleType>				        <xs:restriction base="xs:string">				            <xs:maxLength value="50"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="numPosts" type="xs:long" />				<xs:element name="BoardID" type="xs:long" />				<xs:element name="Avatar"  minOccurs="0" msprop:nullValue="_null">				    <xs:simpleType>				        <xs:restriction base="xs:string">				            <xs:maxLength value="255"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="isApproved" type="xs:boolean"  minOccurs="0"/>				<xs:element name="suspended" type="xs:dateTime"  minOccurs="0"/>				<xs:element name="isActiveExcluded" type="xs:boolean"  minOccurs="0"/>				<xs:element name="AvatarImageType"  minOccurs="0" msprop:nullValue="_null">				    <xs:simpleType>				        <xs:restriction base="xs:string">				            <xs:maxLength value="50"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="IP"  minOccurs="0" msprop:nullValue="_null">				    <xs:simpleType>				        <xs:restriction base="xs:string">				            <xs:maxLength value="15"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="RankID" type="xs:long" />				<xs:element name="flags" type="xs:int" />				<xs:element name="pmNotification" type="xs:boolean" />				<xs:element name="points" type="xs:int" />				<xs:element name="OverrideDefaultTheme" type="xs:boolean" />				<xs:element name="UserID" msdata:ReadOnly="true" msdata:AutoIncrement="true" type="xs:long" />				<xs:element name="AvatarImage" type="xs:base64Binary"  minOccurs="0"/>				<xs:element name="Name" >				    <xs:simpleType>				        <xs:restriction base="xs:string" >				            <xs:maxLength value="50"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="ProfileID" type="xs:long"  minOccurs="0"/>				<xs:element name="LanguageFile"  minOccurs="0" msprop:nullValue="_null">				    <xs:simpleType>				        <xs:restriction base="xs:string">				            <xs:maxLength value="50"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="LastVisit" type="xs:dateTime" />				<xs:element name="ProviderUserKey"  minOccurs="0" msprop:nullValue="_null">				    <xs:simpleType>				        <xs:restriction base="xs:string">				            <xs:maxLength value="64"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>						</xs:sequence>					</xs:complexType>				</xs:element>			</xs:choice>		</xs:complexType>		<xs:unique name="PKForumsUser" msdata:PrimaryKey="true">			<xs:selector xpath=".//mstns:ForumsUser" />			<xs:field xpath="mstns:UserID" />		</xs:unique>	</xs:element></xs:schema>