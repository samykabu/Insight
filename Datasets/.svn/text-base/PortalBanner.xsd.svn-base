<?xml version="1.0" encoding="utf-8" ?><xs:schema id="PortalBannerDS" targetNamespace="http://tempuri.org/PortalBannerDS.xsd" elementFormDefault="qualified"	attributeFormDefault="qualified" xmlns="http://tempuri.org/PortalBannerDS.xsd" xmlns:mstns="http://tempuri.org/PortalBannerDS.xsd"	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" xmlns:msprop="urn:schemas-microsoft-com:xml-msprop" >	<xs:element name="PortalBannerDS" msdata:IsDataSet="true">		<xs:complexType>			<xs:choice maxOccurs="unbounded">				<xs:element name="PortalBanner">					<xs:complexType>						<xs:sequence>				<xs:element name="BannerType" type="xs:int" />				<xs:element name="BannerID" msdata:ReadOnly="true" msdata:AutoIncrement="true" type="xs:long" />				<xs:element name="Title" >				    <xs:simpleType>				        <xs:restriction base="xs:string" >				            <xs:maxLength value="150"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="StartIn" type="xs:dateTime" />				<xs:element name="ExpireIn" type="xs:dateTime"  minOccurs="0"/>				<xs:element name="Width" type="xs:int" />				<xs:element name="ClickURL"  minOccurs="0" msprop:nullValue="_null">				    <xs:simpleType>				        <xs:restriction base="xs:string">				            <xs:maxLength value="250"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="Height" type="xs:int" />				<xs:element name="BannerImage" type="xs:base64Binary" />						</xs:sequence>					</xs:complexType>				</xs:element>			</xs:choice>		</xs:complexType>		<xs:unique name="PKPortalBanner" msdata:PrimaryKey="true">			<xs:selector xpath=".//mstns:PortalBanner" />			<xs:field xpath="mstns:BannerID" />		</xs:unique>	</xs:element></xs:schema>