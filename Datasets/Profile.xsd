<?xml version="1.0" encoding="utf-8" ?><xs:schema id="ProfileDS" targetNamespace="http://tempuri.org/ProfileDS.xsd" elementFormDefault="qualified"	attributeFormDefault="qualified" xmlns="http://tempuri.org/ProfileDS.xsd" xmlns:mstns="http://tempuri.org/ProfileDS.xsd"	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" xmlns:msprop="urn:schemas-microsoft-com:xml-msprop" >	<xs:element name="ProfileDS" msdata:IsDataSet="true">		<xs:complexType>			<xs:choice maxOccurs="unbounded">				<xs:element name="Profile">					<xs:complexType>						<xs:sequence>				<xs:element name="Mobile1" >				    <xs:simpleType>				        <xs:restriction base="xs:string" >				            <xs:maxLength value="50"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="Password" >				    <xs:simpleType>				        <xs:restriction base="xs:string" >				            <xs:maxLength value="50"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="HomePhone1"  minOccurs="0" msprop:nullValue="_null">				    <xs:simpleType>				        <xs:restriction base="xs:string">				            <xs:maxLength value="50"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="LoginName" >				    <xs:simpleType>				        <xs:restriction base="xs:string" >				            <xs:maxLength value="50"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="EnableNotifications" type="xs:boolean"  minOccurs="0"/>				<xs:element name="Email" >				    <xs:simpleType>				        <xs:restriction base="xs:string" >				            <xs:maxLength value="250"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="LastLogin" type="xs:dateTime"  minOccurs="0"/>				<xs:element name="FileQouta" type="xs:long"  minOccurs="0"/>				<xs:element name="FName" >				    <xs:simpleType>				        <xs:restriction base="xs:string" >				            <xs:maxLength value="50"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="ProfileImage" type="xs:base64Binary"  minOccurs="0"/>				<xs:element name="ProfileID" msdata:ReadOnly="true" msdata:AutoIncrement="true" type="xs:long" />				<xs:element name="LName" >				    <xs:simpleType>				        <xs:restriction base="xs:string" >				            <xs:maxLength value="50"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="SecurityRoles" type="xs:int" />				<xs:element name="isPortalAdmin" type="xs:boolean" />						</xs:sequence>					</xs:complexType>				</xs:element>			</xs:choice>		</xs:complexType>		<xs:unique name="PKProfile" msdata:PrimaryKey="true">			<xs:selector xpath=".//mstns:Profile" />			<xs:field xpath="mstns:ProfileID" />		</xs:unique>	</xs:element></xs:schema>