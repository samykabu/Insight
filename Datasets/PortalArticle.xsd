<?xml version="1.0" encoding="utf-8" ?><xs:schema id="PortalArticleDS" targetNamespace="http://tempuri.org/PortalArticleDS.xsd" elementFormDefault="qualified"	attributeFormDefault="qualified" xmlns="http://tempuri.org/PortalArticleDS.xsd" xmlns:mstns="http://tempuri.org/PortalArticleDS.xsd"	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" xmlns:msprop="urn:schemas-microsoft-com:xml-msprop" >	<xs:element name="PortalArticleDS" msdata:IsDataSet="true">		<xs:complexType>			<xs:choice maxOccurs="unbounded">				<xs:element name="PortalArticle">					<xs:complexType>						<xs:sequence>				<xs:element name="ApprovedIN" type="xs:dateTime"  minOccurs="0"/>				<xs:element name="Description"  minOccurs="0" msprop:nullValue="_null">				    <xs:simpleType>				        <xs:restriction base="xs:string">				            <xs:maxLength value="450"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="DefLangArticleID" type="xs:long"  minOccurs="0"/>				<xs:element name="isCanceled" type="xs:boolean" />				<xs:element name="Title" >				    <xs:simpleType>				        <xs:restriction base="xs:string" >				            <xs:maxLength value="200"/>				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="Rate" type="xs:int" />				<xs:element name="LanguageID" type="xs:long" />				<xs:element name="CreatedIN" type="xs:dateTime" />				<xs:element name="RawBody"  minOccurs="0" msprop:nullValue="_null">				    <xs:simpleType>				        <xs:restriction base="xs:string">				        </xs:restriction>				    </xs:simpleType>				</xs:element>				<xs:element name="Status" type="xs:int" />				<xs:element name="isPublished" type="xs:boolean" />				<xs:element name="PublishedIn" type="xs:dateTime"  minOccurs="0"/>				<xs:element name="PortalArticleID" msdata:ReadOnly="true" msdata:AutoIncrement="true" type="xs:long" />				<xs:element name="LastEditedIn" type="xs:dateTime"  minOccurs="0"/>						</xs:sequence>					</xs:complexType>				</xs:element>			</xs:choice>		</xs:complexType>		<xs:unique name="PKPortalArticle" msdata:PrimaryKey="true">			<xs:selector xpath=".//mstns:PortalArticle" />			<xs:field xpath="mstns:PortalArticleID" />		</xs:unique>	</xs:element></xs:schema>